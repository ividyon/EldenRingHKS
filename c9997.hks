                    return TRUE
                else
                    return FALSE
                end
            elseif IsExistAnime(ANIME_ID_DAMAGE_WEAK) == TRUE then
                SetVariable("IndexDamageDirection", DIRECTION_FORWARD)
                act(SetDamageAnimType, DAMAGE_FLAG_WEAK)
                Replanning()
                Fire("W_DamageWeak")
