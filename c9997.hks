    end
    if damage_type >= DAMAGE_TYPE_GUARDED_BOUND and damage_type <= DAMAGE_TYPE_GUARDED_BOUND_ENEMY3 or damage_type >= DAMAGE_TYPE_WALL_ENEMY1 and damage_type <= DAMAGE_TYPE_MAP_BOUND or damage_type_under >= DAMAGE_TYPE_GUARDED_BOUND and damage_type_under <= DAMAGE_TYPE_GUARDED_BOUND_ENEMY3 or damage_type_under >= DAMAGE_TYPE_WALL_ENEMY1 and damage_type_under <= DAMAGE_TYPE_MAP_BOUND then
        return FALSE
