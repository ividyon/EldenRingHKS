    local damage_type = env(GetReceivedDamageType)

    if damage_type == DAMAGE_TYPE_DEATHIDLE then
        SetVariable("IndexDeathIdleVariation", 0)
        Fire("W_DeathIdleDefault")
        return
    end
    if env(CheckForEventAnimPlaybackRequest) == TRUE then
        return
    end
    if env(IsGenerateAction) == TRUE and IsExistAnime(ANIME_ID_GENERATE) == TRUE then
        Fire("W_Generate")
        return
    end
    if ExecIdleUnique() == TRUE then
        return
    end

    Fire("W_Idle")
    return
end

