    end
    if hp <= 1 and env(GetSpEffectID, 10592) == FALSE and env(GetSpEffectID, SP_EFFECT_NEW_SKELETON_NO_DEAD) == TRUE then
        return TRUE
    end
    if hp <= 1 and env(GetSpEffectID, SP_EFFECT_SKELETON_NO_DEAD) == TRUE then
