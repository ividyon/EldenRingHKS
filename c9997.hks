    elseif (env(GetDamageSpecialAttribute, STATUS_EFFECT_BLOOD) == TRUE or env(GetDamageSpecialAttribute, STATUS_EFFECT_COLD) == TRUE) and env(GetSpEffectID, 5890) == FALSE and env(GetSpEffectID, 5891) == FALSE then
